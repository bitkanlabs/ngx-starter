{
  "name": "k-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "hmr": "yarn start --configuration=hmr",
    "local": "yarn start --configuration=local",
    "dev": "yarn hmr",
    "build:now": "ng build --configuration=now",
    "build:stage": "ng build --configuration=stage",
    "prestart": "yarn build:libs",
    "http-server": "http-server",
    "build:libs": "ng build @ngks/core",
    "build:bundle:web": "yarn build web && ng run web:server:production",
    "webpack:server:web": "webpack --config projects/web/webpack.server.config.js --progress --colors",
    "build:ssr:web": "yarn run build:bundle:web && yarn run webpack:server:web",
    "serve:ssr:web": "node dist/web/server",
    "build:prerender:web": "yarn run build:bundle:web && yarn run webpack:server:web && yarn run generate:prerender:web",
    "generate:prerender:web": "cd dist && node web/prerender",
    "serve:prerender:web": "cd dist/web && serve",
    "extract:i18n:web": "cd projects/web && ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "build:bundle:mobile": "yarn build mobile && ng run mobile:server:production",
    "webpack:server:mobile": "webpack --config projects/mobile/webpack.server.config.js --progress --colors",
    "build:ssr:mobile": "yarn run build:bundle:mobile && yarn run webpack:server:mobile",
    "serve:ssr:mobile": "node dist/mobile/server",
    "build:prerender:mobile": "yarn run build:bundle:mobile && yarn run webpack:server:mobile && yarn run generate:prerender:mobile",
    "generate:prerender:mobile": "cd dist && node mobile/prerender",
    "serve:prerender:mobile": "cd dist/mobile && serve",
    "extract:i18n:mobile": "cd projects/mobie && ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "analyze": "webpack-bundle-analyzer",
    "inspect": "source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.2",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/platform-server": "^6.1.0",
    "@angular/pwa": "^0.7.2",
    "@angular/router": "^6.1.0",
    "@angular/service-worker": "^6.1.0",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-loading-bar/core": "^2.1.2",
    "@ngx-loading-bar/http-client": "^2.1.2",
    "@ngx-loading-bar/router": "^2.1.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@ngxs/form-plugin": "^3.2.0",
    "@ngxs/router-plugin": "^3.2.0",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "@ngxs/websocket-plugin": "^3.2.0",
    "angular-gtag": "^1.0.3",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "ramda": "^0.25.0",
    "raven-js": "^3.26.4",
    "rxjs": "^6.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.2",
    "@angular-devkit/build-ng-packagr": "~0.7.0",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@ngxs/logger-plugin": "^3.2.0",
    "@types/express": "^4.16.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "~10.5.3",
    "@types/ramda": "^0.25.36",
    "codelyzer": "~4.4.2",
    "express": "^4.16.3",
    "http-server": "^0.11.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.4.2",
    "jest-preset-angular": "^5.2.3",
    "karma": "~2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^3.0.6",
    "protractor": "~5.4.0",
    "serve": "^9.4.0",
    "source-map-explorer": "^1.5.0",
    "ts-loader": "^4.4.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.32.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "webpack-bundle-analyzer": "^2.13.1"
  },
  "jest": {
    "projects": [
      "projects/ngks/core",
      "projects/web",
      "projects/mobile"
    ]
  },
  "workspaces": [
    "projects/ngks/core",
    "projects/web",
    "projects/mobile"
  ]
}
