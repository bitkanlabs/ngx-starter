{
  "name": "k-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --stats-json --vendor-chunk",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "hmr": "yarn start --configuration=hmr",
    "local": "yarn start --configuration=local",
    "dev": "yarn hmr",
    "prestart": "yarn build:libs",
    "build:libs": "ng build @ngks/core",
    "build:bundle:web": "yarn build web --build-optimizer && ng run web:server:production",
    "webpack:server:web": "webpack --config projects/web/webpack.server.config.js --progress --colors",
    "build:ssr:web": "yarn run build:bundle:web && yarn run webpack:server:web",
    "serve:ssr:web": "node dist/web-server",
    "build:prerender:web": "yarn run build:bundle:web && yarn run webpack:server:web && yarn run generate:prerender:web",
    "generate:prerender:web": "cd dist && node web-prerender",
    "serve:prerender:web": "cd dist/web && serve",
    "extract:i18n:web": "cd projects/web && ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "build:bundle:mobile": "yarn build mobile --build-optimizer && ng run mobile:server:production",
    "webpack:server:mobile": "webpack --config projects/mobile/webpack.server.config.js --progress --colors",
    "build:ssr:mobile": "yarn run build:bundle:mobile && yarn run webpack:server:mobile",
    "serve:ssr:mobile": "node dist/mobile-server",
    "build:prerender:mobile": "yarn run build:bundle:mobile && yarn run webpack:server:mobile && yarn run generate:prerender:mobile",
    "generate:prerender:mobile": "cd dist && node mobile-prerender",
    "serve:prerender:mobile": "cd dist/mobile && serve",
    "extract:i18n:mobile": "cd projects/mobie && ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker _",
    "analyze": "webpack-bundle-analyzer",
    "inspect": "source-map-explorer"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.7",
    "@angular/cdk": "^6.3.2",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/material": "^6.3.2",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/platform-server": "^6.0.7",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.7",
    "@angular/service-worker": "^6.0.7",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@ngxs/form-plugin": "^3.1.4",
    "@ngxs/router-plugin": "^3.1.4",
    "@ngxs/storage-plugin": "^3.1.4",
    "@ngxs/store": "^3.1.4",
    "@ngxs/websocket-plugin": "^3.1.4",
    "core-js": "^2.5.4",
    "ramda": "^0.25.0",
    "raven-js": "^3.26.3",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular-devkit/build-ng-packagr": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngxs/devtools-plugin": "^3.1.4",
    "@ngxs/logger-plugin": "^3.1.4",
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^23.1.4",
    "@types/node": "~10.1.4",
    "@types/ramda": "0.25.24",
    "codelyzer": "^4.4.2",
    "express": "^4.16.3",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^23.3.0",
    "jest-preset-angular": "^5.2.3",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^3.0.3",
    "protractor": "^5.3.2",
    "serve": "^9.1.2",
    "source-map-explorer": "^1.5.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tsickle": "^0.32.0",
    "tslib": "^1.9.3",
    "tslint": "^5.10.0",
    "typescript": "~2.7.2",
    "webpack": "~4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.3"
  },
  "jest": {
    "projects": [
      "projects/ngks/core",
      "projects/web",
      "projects/mobile"
    ]
  },
  "workspaces": [
    "projects/ngks/core",
    "projects/web",
    "projects/mobile"
  ]
}
